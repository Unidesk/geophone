<?xml version="1.0" encoding="UTF-8"?>
<!--
License key file for geophone

The key can be of any value, though GUIDs generally work nicely since they are difficult to guess.

There can be any number of license keys defined.
-->


<geophone>
    <server>
        <port>8081</port>
    </server>
    <keys>
        <key owner="Example API user">ea84d5bb-15c8-42d3-abb9-2a3e661bef5d</key>
        <key owner="Simple key example">1</key>
    </keys>
</geophone>
